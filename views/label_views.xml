<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="label_form" model="ir.ui.view">
            <field name="name">label.form</field>
            <field name="model">label</field>
            <field name="arch" type="xml">
                <form string="Create Label">
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="project_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="label_tree" model="ir.ui.view">
            <field name="name">label.view</field>
            <field name="model">label</field>
            <field name="arch" type="xml">
                <tree string="Label" editable="bottom">
                    <field name="name"/>
                </tree>
            </field>
        </record>


        <record id="action_label" model="ir.actions.act_window">
            <field name="name">Label</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">label</field>
            <field name="view_mode">tree,form</field>

        </record>

<!--        <menuitem id="label"-->
<!--            name="Label"-->
<!--            action="action_label"-->
<!--            sequence="10"-->
<!--            groups="base.group_user"/>-->
    </data>
</odoo>
